ate($e,eventListItem){\n    def lazy on$e(function $i){\n        INT $e = $e.scoreboard\n        def @inputInit @inputReset$e $eInit(){\n            $e = 0\n        }\n        \n        if ($e > 0){\n            $i()\n            schedule.add(1){\n                with(@a,true){\n                    @inputReset$e()\n                }\n            }\n        }\n    }\n    def lazy on$e(int $data, function $i){\n        INT $e = $e.scoreboard\n        def @inputInit @inputReset$e $eInit(){\n            $e = 0\n        }\n        \n        if ($e > 0){\n            if (inventory.isholdingitem($i.item, $data)){\n                $i()\n            }\n            schedule.add(1){\n                with(@a,true){\n                    @inputReset$e()\n                }\n            }\n        }\n    }\n}\n\nforgenerate($e,eventList){\n    def lazy on$e(function $i){\n        INT $e = $e.scoreboard\n        def @inputInit @inputReset$e $eInit(){\n            $e = 0\n        }\n        \n        if ($e > 0){\n            $i()\n            schedule.add(1){\n                with(@a,true){\n                    @inputReset$e()\n                }\n            }\n        }\n    }\n}\n\nforgenerate($e,eventParamBlockList){\n    def lazy on$e(int $param, function $fct){\n        if (__isblocktags($param)){\n            forgenerate($i, blocktags($param)){\n                INT $e_$i = $e.scoreboard$i\n                def @inputInit @inputReset$e $e_$iInit(){\n                    $e_$i = 0\n                }\n                \n                if ($e_$i > 0){\n                    $fct()\n                    schedule.add(1){\n                        with(@a,true){\n                            @inputReset$e()\n                        }\n                    }\n                }\n            }\n        }\n        else{\n            INT $e_$param = $e.scoreboard$param\n            def @inputInit @inputReset$e $e_$paramInit(){\n                $e_$param = 0\n            }\n            \n            if ($e_$param > 0){\n                $fct()\n                schedule.add(1){\n                    with(@a,true){\n                        @inputReset$e()\n                    }\n                }\n            }\n        }\n    }\n}\n\nforgenerate($e,eventParamItemList){\n    def lazy on$e(int $param, function $fct){\n        if (__isitemtags($param)){\n            forgenerate($i, itemtags($param)){\n                INT $e_$i = $e.scoreboard$i\n                def @inputInit @inputReset$e $e_$iInit(){\n                    $e_$i = 0\n                }\n                \n                if ($e_$i > 0){\n                    $fct()\n                    schedule.add(1){\n                        with(@a,true){\n                            @inputReset$e()\n                        }\n                    }\n                }\n            }\n        }\n        else{\n            INT $e_$param = $e.scoreboard$param\n            def @inputInit @inputReset$e $e_$paramInit(){\n                $e_$param = 0\n            }\n            \n            if ($e_$param > 0){\n                $fct()\n                schedule.add(1){\n                    with(@a,true){\n                        @inputReset$e()\n                    }\n                }\n            }\n        }\n    }\n}\n\nforgenerate($e,eventParamEntityList){\n    def lazy on$e(int $param, function $fct){\n        if (__isentitytags($param)){\n            forgenerate($i, entitytags($param)){\n                INT $e_$i = $e.scoreboard$i\n                def @inputInit @inputReset$e $e_$iInit(){\n                    $e_$i = 0\n                }\n                \n                if ($e_$i > 0){\n                    $fct()\n                    schedule.add(1){\n                        with(@a,true){\n                            @inputReset$e()\n                        }\n                    }\n                }\n            }\n        }\n        else{\n            INT $e_$param = $e.scoreboard$param\n            def @inputInit @inputReset$e $e_$paramInit(){\n                $e_$param = 0\n            }\n            \n            if ($e_$param > 0){\n                $fct()\n                schedule.add(1){\n                    with(@a,true){\n                        @inputReset$e()\n                    }\n                }\n            }\n        }\n    }\n}\n\nforgenerate($e,continuousEventList){\n    def lazy on$e(function $i){\n        INT $e = $e.scoreboard\n        INT $ePrev\n        $ePrev:=0\n        def @inputInit @inputReset$e $eInit(){\n            $e = 0\n        }\n        if ($e > 0 && $ePrev == 0){\n            $i()\n            $ePrev = 1\n        }\n        if ($e == 0 && $ePrev == 1){\n            $ePrev = 0\n        }\n        $eInit()\n    }\n    def lazy while$e(function $i){\n        INT $eCont = $e.scoreboard\n        \n        if ($eCont > 0){\n            $i()\n            schedule.add(1){\n                with(@a,true){\n                    @inputReset$e()\n                }\n            }\n        }\n        \n        def @inputInit @inputReset$e $eInit(){\n            $eCont = 0\n        }\n    }\n}","index":0},{"name":"button","content":"package input\n\nprivate enum button(json funcname){\n    stone_button(stonebutton),\n    oak_button(oakbutton),\n    birch_button(birchbutton),\n    spruce_button(sprucebutton),\n    dark_oak_button(darkoakbutton),\n    acacia_button(acaciabutton),\n    jungle_button(junglebutton),\n    polished_blackstone_button(blackstonebutton),\n    warped_button(warpedbutton),\n    crimson_button(crimsonbutton)\n}\nprivate enum button_facing = north, east, west, south\nprivate enum button_face = floor,wall,ceiling\n\nforgenerate($button,button){\n    def lazy on$button.funcname(int $x, int $y, int $z,function $func){\n        positioned($x $y $z){\n            if (block(~ ~ ~ minecraft:$button[powered=true])){\n                $func()\n                forgenerate($facing,button_facing){\n                    forgenerate($face,button_face){\n                        if (block(~ ~ ~ minecraft:$button[face=$face,facing=$facing,powered=true])){\n                            /setblock ~ ~ ~ minecraft:$button[face=$face,facing=$facing,powered=false]\n                        }\n                    }\n                }\n            }\n        }\n    }\n}","index":0},{"name":"wasd","content":"package input\n\npublic struct MotionController{\n    float player_x\n    float player_y\n    float player_z\n    bool active\n    void=>void XPlusEvent\n    void=>void XMinusEvent\n    void=>void ZPlusEvent\n    void=>void ZMinusEvent\n    \n    def __init__(bool init = false, void=>void onXPlus = null,\n        void=>void onXMinus = null, void=>void onZPlus = null,\n    void=>void onZMinus = null){\n        XPlusEvent = onXPlus\n        XMinusEvent = onXMinus\n        ZPlusEvent = onZPlus\n        ZMinusEvent = onZMinus\n        if (init){\n            start()\n        }\n    }\n    \n    public void start(){\n        player_x, player_y, player_z = @s.x, @s.y, @s.z\n        active = true\n    }\n    public void stop(){\n        active = false\n    }\n    public lazy tick(){\n        motioncontroll()\n    }\n    public void motioncontroll(){\n        float nx,ny,nz = vector.position()\n        float ax = math.abs(nx-player_x)\n        float az = math.abs(nz-player_z)\n        if (ax > az){\n            if (nx < player_x){\n                XMinusEvent()\n                TPBack()\n            }\n            if (nx > player_x){\n                XPlusEvent()\n                TPBack()\n            }\n        }\n        if (az > ax){\n            if (nz < player_z){\n                ZMinusEvent()\n                TPBack()\n            }\n            if (nz > player_z){\n                ZPlusEvent()\n                TPBack()\n            }\n        }\n    }\n    \n    public void TPBack(){\n        at(player_x, player_y, player_z){\n            /tp @s ~ ~ ~\n        }\n    }\n}","index":0},{"name":"pressure","content":"package input\n\nprivate enum pressure_plate(json funcname){\n    stone_pressure_plate(stonepressureplate),\n    oak_pressure_plate(oakpressureplate),\n    birch_pressure_plate(birchpressureplate),\n    spruce_pressure_plate(sprucepressureplate),\n    dark_oak_pressure_plate(darkoakpressureplate),\n    acacia_pressure_plate(acaciapressureplate),\n    jungle_pressure_plate(junglepressureplate),\n    polished_blackstone_pressure_plate(blackstonepressureplate),\n    warped_pressure_plate(warpedpressureplate),\n    crimson_pressure_plate(crimsonpressureplate),\n}\nprivate enum weighted_pressure_plate(json funcname){\n    heavy_weighted_pressure_plate(heavyweightedpressureplate),\n    light_weighted_pressure_plate(lightweightedpressureplate)\n}\n\nforgenerate($pressure_plate,pressure_plate){\n    def lazy on$pressure_plate.funcname(int $x, int $y, int $z,function $func){\n        bool on := false\n        if (block($x $y $z minecraft:$pressure_plate[powered=true]) && !on){\n            $func()\n            on = true\n        }\n        else{\n            on = false\n        }\n    }\n}\n\nforgenerate($pressure_plate,weighted_pressure_plate){\n    def lazy on$pressure_plate.funcname(int $x, int $y, int $z,function $func){\n        bool on := false\n        forgenerate($i,1,15){\n            if (block($x $y $z minecraft:$pressure_plate[power=$i]) && !on){\n                $func()\n                on = true\n            }\n        }\n        if (block($x $y $z minecraft:$pressure_plate[power=0]) && on){\n            on = false\n        }\n    }\n}","index":0}],"resources":[],"TagsList":{"blocks":{},"functions":{}},"mcTagsList":{"blocks":{},"functions":{"load":{"values":["load"]},"tick":{"values":["main"]}}},"compileOrder":[],"version":{"major":1,"minor":0,"patch":0,"build":43},"compilationSetting":{"TreeMaxSize":20,"FloatPrecision":1000,"removeUselessFile":true,"offuscate":true,"randomLambdaID":false,"scoreboardValue":"tbms.value","structuresSource":"","structuresSources":{},"s